<div class="container">

<h2 class="page-header">Enter Beer</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<form method="post" action="/dashboard/beer">
   <div class="form-group">
    <label>Beer Name</label>
    <input type="text" class="form-control" placeholder="Beer Name" name="beername">
  </div>
  <div class="form-group">
    <label>Brewery Name</label>
    <input type="text" class="form-control" placeholder="Brewery Name" name="breweryname">
  </div>
  <div class="form-group">
    <label>Country of Origin</label>
    <input type="text" class="form-control" placeholder="Country of Origin" name="origin">
  </div>
  
   <div class="form-group">
    <label>Type of Beer</label>
    <input type="text" class="form-control" placeholder="Type of Beer" name="type">
  </div>
  {{!-- <div class="form-group">
    <label>Password</label>
    <input type="password" class="form-control" placeholder="Password" name="password">
  </div>
  <div class="form-group">
    <label>Confirm Password</label>
    <input type="password" class="form-control" placeholder="Password" name="password2">
  </div> --}}
  <button type="submit" class="btn btn-default">Submit</button>
</form>

</div>