<div class="container body-container row">
    <div class="col s12 m12">
        <form>
            <center>
                <div class="row">
                    <div class='input-field col s12'>
                        <br>
                        <br>
                        <table>
                            <thead>
                                <th>Beer</th>
                                <th>Brewer</th>
                                <th>Origin</th>
                                <th>Type</th>
                            </thead>
                            <tbody>
                                {{#each beer}}
                                <tr>
                                    <td>{{this.beername}}</td>
                                    <td>{{this.breweryname}}</td>
                                    <td>{{this.origin}}</td>
                                    <td>{{this.type}}</td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </center>
        </form>
    </div>
</div>
